<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>kinova_description</name>
  <version>0.0.1</version>
  <description>
      3D model of various Kinova robotic arms.
      Includes the model and URDF files for working with an arm in RViz or Gazebo.
  </description>
  <maintainer email="ros@kinovarobotics.com">Kinova ROS Support</maintainer>
  <author>Kinova ROS Support</author>
  <license>BSD</license>
  <url>http://ros.org/wiki/kinova_description</url>

  <buildtool_depend condition="$ROS_VERSION == 2">ament_cmake</buildtool_depend>
  <buildtool_depend condition="$ROS_VERSION == 1">catkin</buildtool_depend>

  <export>
    <build_type condition="$ROS_VERSION == 2">ament_cmake</build_type>
    <build_type condition="$ROS_VERSION == 1">catkin</build_type>
  </export>

</package>


